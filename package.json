{
  "name": "classicube-script",
  "displayName": "ClassiCube Script",
  "description": "ClassiCube Script language support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
